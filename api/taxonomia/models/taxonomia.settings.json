{
  "kind": "collectionType",
  "collectionName": "taxonomias",
  "info": {
    "name": "taxonomia"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "tax_nombre": {
      "type": "string",
      "maxLength": 200
    },
    "tax_id_padre": {
      "type": "integer"
    },
    "tax_id_categoria": {
      "model": "categoria",
      "via": "taxonomias"
    },
    "tax_nombre_cientifico": {
      "type": "string",
      "maxLength": 150
    },
    "registro_especimen": {
      "via": "reg_id_taxa",
      "collection": "registro-especimen"
    },
    "filo": {
      "via": "reg_filo",
      "collection": "registro-especimen"
    },
    "clase": {
      "via": "reg_clase",
      "collection": "registro-especimen"
    },
    "subclase": {
      "via": "reg_subclase",
      "collection": "registro-especimen"
    },
    "orden": {
      "via": "reg_orden",
      "collection": "registro-especimen"
    },
    "especie": {
      "via": "reg_especie",
      "collection": "registro-especimen"
    },
    "genero": {
      "via": "reg_genero",
      "collection": "registro-especimen"
    },
    "subgenero": {
      "via": "reg_subgenero",
      "collection": "registro-especimen"
    },
    "epiteto": {
      "via": "reg_epiteto",
      "collection": "registro-especimen"
    }
  }
}
